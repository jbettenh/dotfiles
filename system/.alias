#

# Directory listing/traversal
eap='/srv/jboss/jboss-eap-7.2'
bel='/standalone/log/gepard/backend-services'

alias cdsrv='cd $eap/standalone/log'
alias cdbe='cd $eap$bel'

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."


# Miscellaneous
alias h="history"
alias ll="ls -a -l"
alias lg="ls -a -l | grep"
alias lt="ls -a -l -t | head -"
alias week="date +%V"
alias x="exit"
